<Window x:Name="wind_progress" x:Class="SplitTool.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SplitTool"
        mc:Ignorable="d" Height="150" Width="400" Title="SplitTool_progressBar">
    <Grid>
        <ProgressBar x:Name="progbar_write" HorizontalAlignment="Center" Height="28" Margin="0,25,0,0" VerticalAlignment="Top" Width="356"/>
        <Button x:Name="btn_ok" Content="OK" HorizontalAlignment="Center" Height="20" Margin="0,82,0,0" VerticalAlignment="Top" Width="50" IsEnabled="False" Click="Button_Click"/>

    </Grid>
</Window>
